package com;

public class CalculatorImpl implements Cal {
	@Override
	public String add(int a,int b) {
		return "sum "+(a+b);
	}
	@Override
	public String sub(int a,int b) {
		return "sub "+(a-b);
	}
	@Override
	public String mul(int a,int b) {
		return "mul "+(a*b);
	}
	@Override
	public String div(int a,int b) {
		return "div "+(a/b);
	}
	@Override
	public String displayErrorMessage() {
		return "Syntax Error";
	}
}
